{
    "$schema": "https://hst.sh/raw/hapovibiwu",
    "applicationId": "rocks.ctjs.ide",
    "applicationName": "Ct.js game engine",
    "description": "Ct.js game engine integrated development environment",
    "copyright": "MIT licensed by CoMiGo Games",
    "author": "CoMiGo Games ct.js and contributors",
    "version": "5.2.0",
    "defaultMode": "window",
    "port": 0,
    "documentRoot": "/app/",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "enableExtensions": true,
    "exportAuthInfo": false,
    "tokenSecurity": "one-time",
    "globalVariables": {},
    "logging": {
        "enabled": true,
        "writeToLogFile": true
    },
    "nativeAllowList": [
        "app.*",
        "server.*",
        "os.*",
        "window.*",
        "events.*",
        "extensions.*",
        "filesystem.*",
        "debug.log",
        "storage.*",
        "clipboard.*",
        "resources.*",
        "computer.getMemoryInfo",
        "computer.getOSInfo"
    ],
    "modes": {
        "window": {
            "title": "Ct.js",
            "width": 800,
            "height": 500,
            "minWidth": 400,
            "minHeight": 200,
            "center": true,
            "fullScreen": false,
            "alwaysOnTop": false,
            "icon": "/app/ct_ide.png",
            "enableInspector": false,
            "borderless": false,
            "maximize": true,
            "hidden": false,
            "resizable": true,
            "exitProcessOnClose": false
        }
    },
    "cli": {
        "binaryName": "ctjs",
        "resourcesPath": "/app/",
        "extensionsPath": "/bgServices/",
        "binaryVersion": "5.5.0",
        "clientVersion": "5.5.0",
        "clientLibrary": "/neutralinoClient/neutralino.js",
        "distributionPath": "/build",
        "autoReloadExclude": "\\.css$"
    }
}
